package fatec.string;

import java.util.Scanner;

public class ContaVogal {
	public static void main(String[]args) {
	
	 Scanner entrada = new Scanner(System.in);
		
	 String[] nome;
	 nome = new String[1];
	 
	 int i = 0;
	 
	 for(i = 0; i < 1; i ++) {
		 System.out.print("Digite o seu nome: ");
		 nome[i] = entrada.next();
	 
	 }
	 
	 
	 entrada.close();
	 
	}
}
